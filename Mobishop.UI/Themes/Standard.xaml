<?xml version="1.0" encoding="UTF-8"?>
<ResourceDictionary
	xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="Mobishop.UI.Themes.Standard">
	<OnPlatform
		x:Key="fontFamilyLight"
		x:TypeArguments="x:String"
		iOS="HelveticaNeue-Light"
		Android="sans-serif-light" />
	<OnPlatform
		x:Key="fontFamilyRegular"
		x:TypeArguments="x:String"
		iOS="HelveticaNeue"
		Android="sans-serif" />
	<OnPlatform
		x:Key="fontFamilyMedium"
		x:TypeArguments="x:String"
		iOS="HelveticaNeue-Medium"
		Android="sans-serif-medium" />
	<OnPlatform
		x:Key="fontFamilyBold"
		x:TypeArguments="x:String"
		iOS="HelveticaNeue-Bold"
		Android="sans-serif-black" />
	<Color
		x:Key="textColor">
		#4d4d4d
	</Color>
	<Color
		x:Key="separatorColor">
		#cccccc
	</Color>
	<Color
		x:Key="headerBackgroundColor">
		#f5f5f5
	</Color>
</ResourceDictionary>